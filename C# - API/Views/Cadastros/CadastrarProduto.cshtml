@{
}

@using (Html.BeginForm("CadastrarProduto", "Cadastros", FormMethod.Post))
{
    @Html.DisplayNameFor(m => m.Tipo)
    <br />
    @Html.TextBoxFor(m => m.Tipo)
    <br />
    @Html.DisplayNameFor(m => m.Valor)
    <br />
    @Html.TextBoxFor(m => m.Valor)
    <br />
    @Html.DisplayNameFor(m => m.StatusRastreamento)
    <br />
    @Html.TextBoxFor(m => m.StatusRastreamento)
    <br />

    <input type="submit" value="Cadastrar" />
}