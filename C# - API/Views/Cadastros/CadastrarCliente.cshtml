@using Rastreio.Models
@model Rastreio.Models.Cliente

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

@using (Html.BeginForm("CadastrarCliente", "Cadastros", FormMethod.Post))
{
    @Html.DisplayNameFor(m => m.Nome) <br />
    @Html.TextBoxFor(m => m.Nome) <br />

    @Html.DisplayNameFor(m => m.Cpf)
    <br />
    @Html.TextBoxFor(m => m.Cpf)
    <br />
    @Html.DisplayNameFor(m => m.Endereco)
    <br />
    @Html.TextBoxFor(m => m.Endereco)
    <br />

    <input type = "submit" value="Cadastrar" />
}