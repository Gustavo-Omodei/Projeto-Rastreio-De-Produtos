@using Rastreio.Models
@model Rastreio.Models.Rastreamento

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
@using (Html.BeginForm("RastrearProdutos", "Rastreio", FormMethod.Get))
{
    Rastreamento rastreio = new Rastreamento();

    @Html.DisplayNameFor(m => m.Id)
    <br />
    @Html.TextBoxFor(m => m.DataEvento)
    <br />

    @Html.DisplayNameFor(m => m.StatusEvento)
    <br />
    @Html.Label(rastreio.StatusEvento)
    <br />
    @Html.DisplayNameFor(m => m.DestinoFinal)
    <br />
    <br />

    <input type="submit" value="Cadastrar" />
}

